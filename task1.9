package com.company;
import java.util.Scanner;
class Scratch {
    public static void main(String[] args) {
        Scanner in=new Scanner(System.in);
        System.out.print("Amount of elements: ");
        int n=in.nextInt();
        int[] array=new int [n];
        for (int i=0;i<n;i++) {
            System.out.print("Insert the " + i + "st element ");
            array[i]=in.nextInt();
        }
        System.out.print("Rezult= "+ sumOfCubes(array));
    }
    public static int sumOfCubes(int[] arr)
    {
        int sum=0;
        for (int i=0;i<arr.length;i++) {
            sum += Math.pow(arr[i], 3);
        }
        return sum;
    }
}
